<?xml version="1.0" encoding="utf-8"?>

<FrameLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.junjunguo.phialmaps.activities.MapActivity">

    <org.oscim.android.MapView
        android:id="@+id/offline_mapview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"></org.oscim.android.MapView>
    <LinearLayout
        android:id="@+id/mapbox_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"></LinearLayout>

    <LinearLayout
        android:id="@+id/custom_map_view_layout"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"></LinearLayout>
</FrameLayout>
